                  </div> 

                <textarea class="form-control prescription"  rows="4"></textarea>
                <section>

                </section>
              </div>

              <div class="form-group">
                
                <div class="ico">
                   <label for="exampleFormControlTextarea1">Advice</label>
                   <a href="">  <i class="fas fa-microphone float-right btn" ></i> </a>
                    <a href=""><i class="fas fa-edit float-right btn"></i></a>
